{
  "request": {
    "method": "GET",
    "urlPath": "/api/v1/event/score",
    "queryParameters": {
      "event_id": {
        "matches": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
      }
    }
  },

  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "event_score_response.json",
    "transformers": ["response-template"]
  }
}